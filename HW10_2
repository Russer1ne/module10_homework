x = undefined
type = typeof(x)

switch (type) {
    
  case ('string'):
    console.log('x - строка');
    break;
  case ('boolean'):
    console.log('x - логическая переменная');
    break;
  case ('number'):
    console.log('x - число');
    break; 
  default:
    console.log('тип x не оределен');
      
}
